<?xml version="1.0"?>
<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
		<!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
		<!ATTLIST VARIABLE TYPE (nature|decision|utility) "nature">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>

<BIF VERSION="0.3">
<NETWORK>
<NAME>BayesNet</NAME>
<VARIABLE TYPE="nature">
	<NAME>`action(item20)`</NAME>
	<OUTCOME>0</OUTCOME>
	<OUTCOME>1</OUTCOME>
</VARIABLE>
<VARIABLE TYPE="nature">
	<NAME>`drama(item20)`</NAME>
	<OUTCOME>0</OUTCOME>
	<OUTCOME>1</OUTCOME>
</VARIABLE>
<VARIABLE TYPE="nature">
	<NAME>`horror(item20)`</NAME>
	<OUTCOME>0</OUTCOME>
	<OUTCOME>1</OUTCOME>
</VARIABLE>
<DEFINITION>
	<FOR>`action(item20)`</FOR>
</DEFINITION>
<DEFINITION>
	<FOR>`drama(item20)`</FOR>
	<GIVEN>`horror(item20)`</GIVEN>
	<GIVEN>`action(item20)`</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>`horror(item20)`</FOR>
</DEFINITION>
</NETWORK>
</BIF>
