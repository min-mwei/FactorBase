<VARIABLE TYPE="nature">
	<NAME>ranking(student0)</NAME>
</VARIABLE>
<VARIABLE TYPE="nature">
	<NAME>rating(course0)</NAME>
</VARIABLE>
<DEFINITION>
	<FOR>diff(course0)</FOR>
	<GIVEN>diff(course0)</GIVEN>
	<GIVEN>rating(course0)</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>intelligence(student0)</FOR>
	<GIVEN>intelligence(student0)</GIVEN>
	<GIVEN>ranking(student0)</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>ranking(student0)</FOR>
	<GIVEN>ranking(student0)</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>rating(course0)</FOR>
	<GIVEN>diff(course0)</GIVEN>
	<GIVEN>rating(course0)</GIVEN>
</DEFINITION>
</NETWORK>
</BIF>
